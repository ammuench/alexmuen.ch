---
import SiteFooter from "@components/SiteFooter.astro";

import LandingLayout from "../layouts/LandingLayout.astro";
---

<LandingLayout>
  <script>
    const scrollAdvanceBtn = document.querySelector("#hero-advance");
    scrollAdvanceBtn?.addEventListener("click", () => {
      const aboutBlock = document.querySelector("#about-me");
      if (aboutBlock) {
        aboutBlock.scrollIntoView({ behavior: "smooth" });
      }
    });
  </script>
  <style>
    .panel-left {
      display: grid;
      grid-template-areas: "icon copy";
      grid-template-columns: 150px 1fr;

      @media (max-width: 600px) {
        grid-template-areas:
          "icon"
          "copy";
        grid-template-columns: 1fr;
        grid-template-rows: 75px 1fr;
      }
    }
    .panel-right {
      display: grid;
      grid-template-areas: "copy icon";
      grid-template-columns: 1fr 150px;

      @media (max-width: 600px) {
        grid-template-areas:
          "icon"
          "copy";
        grid-template-columns: 1fr;
        grid-template-rows: 75px 1fr;
      }
    }

    .grid-panel-icon {
      grid-area: icon;
    }

    .grid-panel-icon > svg {
      @media (max-width: 600px) {
        max-height: 75px;
      }
    }
    .grid-panel-copy {
      grid-area: copy;

      @media (max-width: 600px) {
        text-align: left;
      }
    }

    .grid-panel-copy .btn {
      @media (max-width: 600px) {
        width: 100%;
      }
    }
  </style>
